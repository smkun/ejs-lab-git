<%- include('./partials/head', { title: 'Page Title' }) %>
<body>
  <%- include('./partials/nav') %>
  <h1><%= category %></h1>
  <% menuItems.forEach(item => { %>
    <h3><%= item.name %></h3>
    <p>Price: $<%= item.price.toFixed(2) %></p>
    <p>Rating: <%= item.rating %></p>
    <p><%= item.details %></p>
  <% }); %>
</body>
</html>